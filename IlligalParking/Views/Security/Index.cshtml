@model IEnumerable<Vehicle>
@{
    ViewData["Title"] = "Vehicles";

}

<

<div class="card">
    <div class="card-header">
        <h5 class="card-title">Vehicles</h5>
    </div>

    @if (Model.Count() > 0)
    {
        <table class="table table-striped table-hover">
            <thead>
                <tr>
                    <th>Driver Name</th>
                    <td>Phone Number</td>
                    <th>Parking Type</th>
                    <th>Vehicle Registration</th>
                    <th>Action</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.DriverName</td>
                        <td>@item.CellNumber</td>
                        <td>@item.ParkingAssigned</td>
                        <td>@item.Registration</td>
                        <td>
                            <a asp-action="Details" asp-controller="Security" asp-route-UserID="@item.UserID">Details</a> |
                            <a asp-action="Edit" asp-controller="Securiy" asp-route-UserID="@item.UserID">Edit</a> |
                            <a asp-action="Delete" asp-controller="Security" asp-route-UserID="@item.UserID">Delete</a>
                        </td>
                    </tr>
                }

            </tbody>
        </table>
    }
    else
    {
        <p>No Registered Parking Users.</p>
    }
</div>

<a asp-controller="Complain" asp-action="Index">View Reports</a> | <a asp-controller="Security" asp-action="Create">Create</a>